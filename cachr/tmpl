		var socket = io('http://localhost');
		socket.on('update',function(data){
			$("#update").html(data);
		});
		socket.on('log',function(data){
			$("#nfo").append("<p>"+data+"</p>");
		});
		socket.on('done',function(data){
			alert("\o/");
		});
